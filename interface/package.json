{
  "name": "openemr-interface",
  "version": "0.1.0",
  "description": "UI toolkit, theme and component library for OpenEMR",
  "main": "index.js",
  "scripts": {
    "build": "run-s gulp-build storybook-build",
    "dev": "gulp --dev",
    "dev-docs": "run-p storybook gulp-docs",
    "gulp-build": "gulp -b --dev",
    "gulp-docs": "gulp -p 9001 --dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -s ./,../ -p 9001 -c .storybook",
    "storybook-build": "build-storybook -c .storybook -o .out"
  },
  "keywords": [
    "openemr"
  ],
  "author": "d3sandoval",
  "license": "GNU GPL",
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "babel-core": "^6.26.3",
    "browser-sync": "^2.24.4",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-append-prepend": "^1.0.4",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-csso": "^3.0.1",
    "gulp-filelist": "^1.0.0",
    "gulp-if": "^2.0.2",
    "gulp-rename": "^1.2.3",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "run-sequence": "^2.2.1",
    "storybook-readme": "^3.3.0"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
